{
    "name": "minizinc",
    "displayName": "MiniZinc",
    "description": "Language Support for the MiniZinc modelling language",
    "version": "0.0.1",
    "author": "Jip J. Dekker",
    "repository": {
        "type": "git",
        "url": "https://github.com/Dekker1/vscode-minizinc.git"
    },
	"bugs": {
        "url": "https://github.com/Dekker1/vscode-minizinc/issues"
    },
    "publisher": "dekkerone",
    "icon": "minizinc_icon.png",
    "categories": [
        "Languages",
        "Linters"
    ],
    "engines": {
        "vscode": "^1.19.0"
    },
    "contributes": {
        "grammars": [{
            "language": "minizinc",
            "scopeName": "source.mzn",
            "path": "./syntaxes/minizinc.tmLanguage"
        }],
        "languages": [{
            "id": "minizinc",
            "aliases": ["MiniZinc", "minizinc"],
            "extensions": [".mzn",".fzn",".dzn"],
            "configuration": "./language-configuration.json"
        }],
        "snippets": [
            {
                "language": "minizinc",
                "path": "./snippets/minizinc.json"
            }
        ]
    }
}
